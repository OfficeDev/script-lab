    "cibuild": "yarn build",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx ./src",
    "test": "echo 'no tests'",
    "citest": "yarn test"
  },
  "dependencies": {
    "@types/cors": "^2.8.4",
    "@types/request": "^2.48.1",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "eslint-plugin-import": "^2.27.5",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "16.*",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "nodemon": "^1.18.3",
    "ts-loader": "^5.2.0",
    "typescript": "^3.2.4",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1"
  }
