{
  "name": "editor",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "postinstall": "ts-node --project ../../scripts/tsconfig.json scripts/postinstall.ts",
    "start": "react-scripts start",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx ./src",
    "build": "node scripts/build.js",
    "react-scripts-build": "react-scripts --max_old_space_size=4096 build",
    "test": "jest --env=jsdom --verbose",
    "test:watch": "jest --env=jsdom --verbose --watch",
    "test-coverage": "npm run test -- --coverage",
    "show-coverage": "npm run test-coverage && coverage\\lcov-report\\index.html",
    "start-with-no-browser": "set BROWSER=none&&react-scripts start"
  },
  "dependencies_comments": {
    "query-string": "Restricing query-string to version 5.1.1 because later versions (6+) started targeting ES6 rather than ES5, making it not work for the precompile scripts. See more at https://stackoverflow.com/a/49985749/678505"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "clipboard": "^2.0.1",
    "common": "0.0.1",
    "core-js": "^2.6.5",
    "jest-environment-jsdom": "^29.7.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.19",
    "moment": "^2.29.4",
    "monaco-editor": "0.18.1",
    "office-ui-fabric-react": "^6.211.6",
    "query-string": "5.1.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "styled-components": "^4.1.1",
    "typesafe-actions": "^2.0.4",
    "typescript": "^4.9.5",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/clipboard": "^2.0.1",
    "@types/jest": "^26.0.14",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.118",
    "@types/node": "^16.11.7",
    "@types/office-runtime": "^1.0.17",
    "@types/prettier": "^1.13.2",
    "@types/query-string": "^5.1.0",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/styled-components": "^4.1.0",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.7.0",
    "prettier": "^1.15.2",
    "react-scripts": "^5.0.1",
    "react-testing-library": "^5.3.0",
    "redux-devtools": "^3.4.1",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
