    "lodash": "^4.17.19",
    "moment": "^2.29.4",
    "monaco-editor": "0.18.1",
    "office-ui-fabric-react": "^6.211.6",
    "query-string": "5.1.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-saga": "^0.16.0",
