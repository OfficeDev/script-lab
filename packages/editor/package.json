{
  "name": "editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@microsoft/office-js-helpers": "0.7.4",
    "clipboard": "^2.0.1",
    "color": "^3.0.0",
    "connected-react-router": "^4.3.0",
    "core-js": "^2.5.7",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.10",
    "monaco-editor": "^0.13.1",
    "normalizr": "^3.2.4",
    "office-ui-fabric-react": "6.79.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.16.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "semver": "^5.5.1",
    "styled-components": "^3.4.2",
    "typesafe-actions": "^2.0.4",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "copy-monaco": "node scripts/copy-monaco.js",
    "start": "set HTTPS=true&&react-scripts-ts start",
    "build": "node scripts/build.js",
    "build:storybook": "build-storybook -c .storybook -o build-storybook",
    "lint:tslint": "tslint 'src/**/*.ts?(x)'",
    "lint:prettier": "prettier --list-different './**/*.ts?(x)'",
    "lint": "yarn lint:tslint && yarn lint:prettier",
    "react-scripts-ts:build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "test-coverage": "yarn test -- --coverage",
    "show-coverage": "yarn test-coverage && coverage\\lcov-report\\index.html",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "moduleNameMapper": {
      "office-ui-fabric-react/lib/": "office-ui-fabric-react/lib-commonjs/"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^3.4.8",
    "@storybook/addon-actions": "^3.4.4",
    "@storybook/addon-knobs": "^3.4.6",
    "@storybook/addon-links": "^3.4.4",
    "@storybook/addon-options": "^3.4.10",
    "@storybook/addon-storyshots": "^3.4.4",
    "@storybook/addons": "^3.4.4",
    "@storybook/react": "^3.4.4",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.9",
    "@types/office-js": "^0.0.87",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/storybook__addon-storyshots": "^3.4.1",
    "@types/storybook__react": "^3.0.7",
    "@uifabric/jest-serializer-merge-styles": "^6.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "fs-extra": "^6.0.1",
    "jest": "^22.4.3",
    "jest-styled-components": "^6.2.1",
    "prettier": "1.13.6",
    "react-test-renderer": "^16.3.2",
    "redux-devtools": "^3.4.1",
    "shelljs": "^0.8.2",
    "source-map-explorer": "^1.6.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
